package laptopAdvisor;

public class Laptop {
	//field
			private String Brand;
			private String model;
			private double price;
			private String dimensionAndResolution;
			private String monitorTechnology;
			private String cpu;
			private String ram;
			private String weight;
			private String power;
			private String gpu;
			private String Storage;
			private String os;
			private String hyperlink;
			
			private int[] rating = new int [10];
			
			private int score = 0;
			
			private String getBrand() {
				return Brand;
			}

			private void setBrand(String brand) {
				Brand = brand;
			}

			private String getModel() {
				return model;
			}

			private void setModel(String model) {
				this.model = model;
			}

			private double getPrice() {
				return price;
			}

			private void setPrice(double price) {
				this.price = price;
			}

			private String getDimensionAndResolution() {
				return dimensionAndResolution;
			}

			private void setDimensionAndResolution(String dimensionAndResolution) {
				this.dimensionAndResolution = dimensionAndResolution;
			}

			private String getMonitorTechnology() {
				return monitorTechnology;
			}

			private void setMonitorTechnology(String monitorTechnology) {
				this.monitorTechnology = monitorTechnology;
			}

			private String getCpu() {
				return cpu;
			}

			private void setCpu(String cpu) {
				this.cpu = cpu;
			}

			private String getRam() {
				return ram;
			}

			private void setRam(String ram) {
				this.ram = ram;
			}

			private String getWeight() {
				return weight;
			}

			private void setWeight(String weight) {
				this.weight = weight;
			}

			private String getPower() {
				return power;
			}

			private void setPower(String power) {
				this.power = power;
			}

			private String getGpu() {
				return gpu;
			}

			private void setGpu(String gpu) {
				this.gpu = gpu;
			}

			private String getStorage() {
				return Storage;
			}

			private void setStorage(String storage) {
				Storage = storage;
			}

			private String getOs() {
				return os;
			}

			private void setOs(String os) {
				this.os = os;
			}

			private String getHyperlink() {
				return hyperlink;
			}

			private void setHyperlink(String hyperlink) {
				this.hyperlink = hyperlink;
			}
			
			
			 
			
			
}
